{
  "name": "agent-coder",
  "version": "1.0.0",
  "description": "AI agent orchestration platform for automated coding tasks",
  "type": "module",
  "bin": {
    "agent-coder": "./dist/cli/index.js"
  },
  "scripts": {
    "build": "tsup && npm run build:web",
    "build:core": "tsup --entry.core src/core/index.ts --format esm --dts",
    "build:cli": "tsup src/cli/index.ts --format esm",
    "build:web": "cd src/web && npx next build",
    "dev": "tsx src/web/server.ts",
    "dev:next": "cd src/web && npx next dev --port 3000",
    "cli": "tsx src/cli/index.ts",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:studio": "drizzle-kit studio",
    "test": "npx playwright test",
    "test:ui": "npx playwright test --ui"
  },
  "dependencies": {
    "@anthropic-ai/claude-agent-sdk": "^0.2.50",
    "@github/copilot-sdk": "^0.1.25",
    "better-sqlite3": "^11.7.0",
    "chalk": "^5.4.1",
    "commander": "^13.1.0",
    "drizzle-orm": "^0.39.3",
    "execa": "^9.5.2",
    "next": "^15.2.0",
    "openai": "^6.22.0",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-markdown": "^10.1.0",
    "readline": "^1.3.0",
    "remark-gfm": "^4.0.1",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "telegraf": "^4.16.3",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.2",
    "@types/better-sqlite3": "^7.6.12",
    "@types/node": "^22.13.0",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "@types/uuid": "^10.0.0",
    "drizzle-kit": "^0.30.5",
    "tsup": "^8.4.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.3"
  }
}
